<%= form_with url: doctor_appointments_path, method: :get, local: true do %>
  <div class="text-center ">
    <strong> SEARCH </strong> <br>
    <%= select_tag "search",  options_for_select([ "7 days", "14 days" ,"1 month" ]),
      include_blank: true %>
    <div class="actions">
      <%= submit_tag "SEARCH" %>
    </div>
  </div>
<% end %>
